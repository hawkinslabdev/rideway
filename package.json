{
  "name": "rideway",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "docker:build": "docker build -t rideway:latest -f Dockerfile.slim .",
    "docker:run": "docker run -p 3000:3000 -v rideway-data:/app/data -v rideway-uploads:/app/public/uploads rideway:latest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node -r esbuild-register app/lib/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:init": "node -r esbuild-register scripts/init-db.ts",
    "db:setup": "npm run db:push && npm run db:init"
  },
  "dependencies": {
    "@libsql/client": "^0.15.4",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.9.1",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "framer-motion": "^12.10.0",
    "lucide-react": "^0.507.0",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-csv": "^2.2.2",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "shadcn-ui": "^0.9.5",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^19",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5"
  }
}
